{
  "name": "sheaft",
  "private": true,
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "run-p openapi dev:*",
    "dev:routify": "routify",
    "dev:vite": "vite",
    "preview": "vite preview",
    "build": "run-s build:*",
    "build:routify": "routify -b",
    "build:vite": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write ./src/**/*.{js,svelte,html,ts}",
    "lint": "eslint ./src/**/*.{js,ts,svelte}",
    "lint:fix": "eslint --fix ./src/**/*.{js,ts,svelte}",
    "prelint": "npm run format",
    "openapi": "copyfiles -f \"../Sheaft.Web.Api/wwwroot/swagger/v1/*.*\" \"./src/types/openapi\" && typegen ./src/types/openapi/swagger.yaml > ./src/types/api.d.ts"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.43",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^17.0.33",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "autoprefixer": "^10.4.7",
    "copyfiles": "^2.4.1",
    "eslint": "^8.15.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.51.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.0",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.8"
  },
  "dependencies": {
    "@felte/reporter-dom": "^1.1.2",
    "@felte/validator-vest": "^1.0.9",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@roxi/routify": "^2.18.5",
    "axios": "^0.25.0",
    "date-fns": "^2.28.0",
    "felte": "^1.2.2",
    "form-data": "^4.0.0",
    "jimmy-js": "^0.5.1",
    "jwt-decode": "^3.1.2",
    "nanoid": "^3.3.4",
    "openapi-client-axios": "^5.1.2",
    "qs": "^6.11.0",
    "svelte-fa": "^2.4.0",
    "svelte-local-storage-store": "^0.2.6",
    "svelte-select": "^4.4.7",
    "svelte-simple-modal": "^1.3.4",
    "vest": "^4.4.1"
  }
}
